				.MaximizeRestoreButton			=	"Restore Down";
				.CloseButton					=	"Close";
				.HelpButton						=	"Help";
			}							
		}


		// ******* Initialize the Bitmap theme properties to zero to prevent inheritance problems

		with GFC_CtrlTheme_Bitmap_Control_TitleBar
		{
			with .Skin
			{
				.Full_Default						=	0;
