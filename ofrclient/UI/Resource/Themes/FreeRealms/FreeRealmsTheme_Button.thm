				.Rollover						=	.Default;
				.Focus							=	.Default;
				.FocusRollover					=	.Default;
				.Toggle							=	.Default;		
				.TogglePress					=	.Press;			
				.ToggleDisable					=	.Disable;		
				.ToggleRollover					=	.Rollover;		
				.ToggleFocus					=	.Focus;			
				.ToggleFocusRollover			=	.FocusRollover;	
													
